{
  "name": "vscode-open-in-github",
  "displayName": "Open in GitHub!",
  "description": "Jump to a source code line in Github!",
  "icon": "images/icon_200.png",
  "version": "0.1.8",
  "publisher": "ziyasal",
  "license": "SEE LICENSE IN LICENSE.md",
  "galleryBanner": {
    "color": "#5c2d91",
    "theme": "light"
  },
  "homepage": "https://github.com/ziyasal/vscode-open-in-github/blob/master/README.md",
   "repository": {
    "type": "git",
    "url": "https://github.com/ziyasal/vscode-open-in-github.git"
  },
  "bugs": {
    "url": "https://github.com/ziyasal/vscode-open-in-github/issues"
  },  
  "categories": [
		"Other"
	],
  "engines": {
    "vscode": "0.10.x"
  },
  "activationEvents": [
    "onCommand:extension.openInGitHub"
  ],
  "main": "./src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.openInGitHub",
        "title": "Open in GitHub"
      }
    ],
		"keybindings": [{
			"command": "extension.openInGitHub",
			"key": "ctrl+o ctrl+g",
			"mac": "ctrl+o ctrl+g",
      "when":"editorTextFocus"
		}]
  },
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
  },
  "devDependencies": {
    "typescript": "^1.6.2",
     "vscode": "0.10.1"
  },
  "dependencies": {
    "github-url-from-git": "^1.4.0",
    "parse-git-config": "^0.3.1"
  }
}